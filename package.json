{
  "name": "flybook",
  "version": "1.4.4",
  "main": "./dist/app.js",
  "license": "MIT",
  "homepage": "https://rhiokim.github.io/flybook",
  "repository": {
    "url": "https://github.com/rhiokim/flybook.git",
    "type": "git"
  },
  "author": {
    "name": "Rhio Kim",
    "email": "rhio.kim@gmail.com",
    "url": "https://flybook.github.io/"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "npx fly",
    "release": "npx fly release",
    "serve": "npx serve out -s -p 5002",
    "docs": "./dist/bin/cli.js docs --outdir=out --font=Overpass --codeStyle=tomorrow-night",
    "docs:prod": "./dist/bin/cli.js docs --outdir=out --prod --font=Overpass --codeStyle=solarized-light --theme=light",
    "pages": "npx gulp && npm run docs:prod && npx gh-pages -d out",
    "lint": "npx eslint --format=node_modules/eslint-formatter-pretty 'libs/**/*.@(js|jsx)'",
    "lint:msg": "npx validate-commit-msg",
    "format": "npx prettier-eslint --write --print-width 120 'libs/**/*.@(js|jsx)'",
    "cz": "git cz",
    "precommit": "npx lint-staged",
    "commitmsg": "npm run lint:msg",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "babel-register": "^6.24.1",
    "classnames": "^2.2.5",
    "commander": "^2.12.2",
    "deep-assign": "^2.0.0",
    "del": "^3.0.0",
    "eslint-plugin-node": "^5.0.0",
    "fs-extra": "^3.0.1",
    "fs-extra-promise": "^1.0.1",
    "glob": "^7.1.2",
    "gray-matter": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-debug": "^3.1.0",
    "gulp-file": "^0.3.0",
    "gulp-filter": "^5.0.1",
    "gulp-foreach": "^0.1.0",
    "gulp-git": "^2.4.2",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-transform": "^3.0.5",
    "gulp-util": "^3.0.8",
    "inquirer": "^3.1.0",
    "js-yaml": "^3.8.4",
    "json2yaml": "^1.1.0",
    "lodash.pick": "^4.4.0",
    "lodash.zip": "^4.2.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mustache": "^2.3.0",
    "ramda": "^0.25.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "recursive-copy": "^2.0.6",
    "remark": "^7.0.1",
    "remark-autolink-headings": "^5.0.0",
    "remark-highlight.js": "^5.0.0",
    "remark-html": "^6.0.0",
    "remark-slug": "^4.2.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "require-dir": "^0.3.2",
    "through2": "^2.0.3",
    "titleize": "^1.0.0",
    "update-notifier": "^2.2.0",
    "vinyl": "^2.1.0",
    "vinyl-transform": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.0.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-config-standard": "^10.2.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "fly": "^2.0.6",
    "fly-babel": "^2.1.1",
    "fly-clear": "^1.0.1",
    "fly-esnext": "^2.0.1",
    "fly-watch": "^1.1.1",
    "gh-pages": "^1.0.0",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "node-notifier": "^5.1.2",
    "prettier-eslint": "^6.3.0",
    "prettier-eslint-cli": "^4.1.1",
    "replace": "^0.3.0",
    "validate-commit-msg": "^2.12.2"
  },
  "bin": {
    "flybook": "./dist/bin/cli.js"
  },
  "files": [
    "dist",
    "static",
    "index.js"
  ],
  "config": {
    "validate-commit-msg": {
      "preset": "angular",
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "libs/**/*.{js,jsx}": [
      "format",
      "lint",
      "git add"
    ],
    "components/**/*.{js,jsx}": [
      "format",
      "lint",
      "git add"
    ],
    "pages/**/*.{js,jsx}": [
      "format",
      "lint",
      "git add"
    ]
  },
  "contributors": [
    {
      "name": "Uno Kim",
      "email": "djkehh@gmail.com",
      "url": "https://djkeh.github.io/"
    }
  ]
}
